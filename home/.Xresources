! ROFI !
rofi.modi:                           window,drun,run
rofi.sidebar-mode:                   true
rofi.terminal:                       termite
rofi.opacity:                        100
rofi.width:                          50
rofi.lines:                          8
rofi.columns:                        3
! rofi.font:                           Monospace 9
! State:           'bg',     'fg',     'bgalt',  'hlbg',   'hlfg'
rofi.color-normal: #0c0c0c, #d9d9d9, #0c0c0c, #f4005f, #c4c5b5
! rofi.color-normal: #353944,  #FFFFFF,  #353944,  #5294E2,  #FFFFFF
! rofi.color-normal: #353944,  #FFFFFF,  #353944,  #5294E2,  #FFFFFF

!                  'background', 'border'
rofi.color-window: #0c0c0c, #f4005f
rofi.bw:                             5
rofi.location:                       0
rofi.padding:                        5
rofi.levenshtein-sort:               true
rofi.case-sensitive:                 false
rofi.fuzzy:                          false
rofi.line-margin:                    2
rofi.separator-style:                solid
rofi.hide-scrollbar:                 true
rofi.markup-rows:                    false
rofi.scrollbar-width:                8




























































































































































































